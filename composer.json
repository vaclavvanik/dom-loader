{
  "name": "vaclavvanik/dom-loader",
  "description": "Load DOM document safety",
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/vaclavvanik/dom-loader",
  "keywords": [
    "dom",
    "load",
    "xml"
  ],
  "authors": [
    {
      "name": "Václav Vaník",
      "email": "vaclav.vanik@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-dom": "*",
    "ext-libxml": "*"
  },
  "require-dev": {

  },
  "autoload": {
    "psr-4": {
      "VaclavVanik\\DomLoader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VaclavVanikTest\\DomLoader\\": "test/unit"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
